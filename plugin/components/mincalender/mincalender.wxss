.calContainer {
  width: 80%;
  margin: 50rpx auto;
  color: #000;
}
.calWrap {
  width: 96%;
  padding: 10rpx 2%;
  background: #434141;
  border-radius: 6px;
}
.calWrap .calTitle {
  display: block;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #fff;
}
.calPrev ,.calNext {
  display: inline-block;
  width: 32px;
  height: 32px;
  margin: 0 10px;
  text-align: center;
  vertical-align: middle;
}
.calName {
  display: inline-block;
  width: calc(100% - 104px);
  line-height: 90rpx;
  text-align: center;
  color: #fff;
}
.calBody,.calBody nav,.calBody ul {
  display: block;
  overflow: hidden;
  background: #434141;
  border-radius: 5px;
}
.calBody nav a, .calBody ul li{
  float: left;
  width: calc(100% / 7);
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  box-sizing: border-box;
  color: #ccc;
}
.calBody ul li {
  color: #000;
  position: relative;
}
.bgColor {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  height: 70rpx;
  background: #3e99f7;
  border-radius: 5px;
}
.calBody li span {
  position: relative;
  z-index: 2;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
}